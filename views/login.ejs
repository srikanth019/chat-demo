<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>ChatCat - Login</title>
    <link rel="stylesheet" href="./css/loginStyles.css" />
    <script src="/js/jquery-2.1.4.min.js"></script>
    <script src="socket.io/socket.io.js"></script>
  </head>
  <body>
    <div class="container">
      <div class="titlePanel">
        <h1>ChatCAT | Login</h1>
      </div>
      <div class="loginButton">
        <a href="/auth/google">
          <img
            id="loginBtnImg"
            src="./img/google.png"
            alt="Sign-In with Facebook"
            style="padding: 5px; width: 335px; border-radius: 5px"
          />
        </a>
        <a href="/auth/facebook">
          <img
            id="loginBtnImg"
            src="./img/fbLoginBtn.png"
            alt="Sign-In with Facebook"
          />
        </a>

        <a href="/auth/twitter">
          <img
            id="loginBtnImg"
            src="./img/twLoginBtn.png"
            alt="Sign-In with Twitter"
          />
        </a>
      </div>
    </div>
  </body>
</html>
